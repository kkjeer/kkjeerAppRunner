{
  "ver": "0.0.1",
  "authors": ["kkjeer"],
  "contact": "",
  "categories": ["active"],
  "widgets": {
    "input": null,
    "output": null
  },
  "parameters": [
    {
      "id": "select_app",
      "optional": false,
      "field_type": "dropdown",
      "dropdown_options": {
        "options": [
          {
            "value": "kb_Bowtie2",
            "display": "kb_Bowtie2.align_reads_to_assembly_app"
          },
          {
            "value": "fba_tools",
            "display": "fba_tools.run_flux_balance_analysis"
          },
          {
            "value": "CGViewAdvanced",
            "display": "CGViewAdvanced.run_CGViewAdvanced"
          },
          {
            "value": "Weka",
            "display": "Weka.decision_tree"
          }
        ]
      }
    },
    {
      "id": "params_json",
      "optional": false,
      "field_type": "textarea",
      "textarea_options": {
        "n_rows": 8
      }
    }
  ],
  "parameter-groups": [
    {
      "id": "param_group",
      "parameters": ["select_app", "params_json"],
      "optional": true,
      "allow_multiple": true,
      "with_border": true
    }
  ],
  "behavior": {
    "service-mapping": {
      "url": "",
      "name": "kkjeerAppRunner",
      "method": "run_kkjeerAppRunner",
      "input_mapping": [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "narrative_system_variable": "workspace_id",
          "target_property": "workspace_id"
        },
        {
          "input_parameter": "param_group",
          "target_property": "param_group"
        }
      ],
      "output_mapping": [
        {
          "service_method_output_path": [0, "report_name"],
          "target_property": "report_name"
        },
        {
          "service_method_output_path": [0, "report_ref"],
          "target_property": "report_ref"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
